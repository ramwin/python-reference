import threading
import datetime


class A(object):
    def __init__(self, value):
        self.value = value


def p(a):
    print(datetime.datetime.now().isoformat())
    print(a.value)


def m():
    for i in range(10):
        a = A(i)
        if i ==3 :
            timer = threading.Timer(3, p, args=[a], kwargs={})
            timer.start()
        else:
            print(a.value)


m()
